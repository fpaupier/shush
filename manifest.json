{

  "manifest_version": 2,
  "name": "Shush",
  "version": "1.0",

  "description": "Remove the LinkedIn feed",

  "icons": {
    "48": "icons/border-48.png",
    "96": "icons/border-96.png"
  },

  "content_scripts": [
    {
      "css": [
        "shush.css"
      ],
      "matches": [
        "*://*.linkedin.com/*"
      ]
    }
  ]

}